{
  "name": "geokit",
  "version": "0.0.0",
  "private": true,
  "description": "Open-source toolkit for making websites AI-ready",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "version:status": "node scripts/sync-versions.js status",
    "version:bump": "node scripts/sync-versions.js release",
    "prepare": "husky"
  },
  "keywords": [
    "geo",
    "seo",
    "ai",
    "audit",
    "llms-txt",
    "generative-engine-optimization",
    "ai-seo",
    "ai-readiness"
  ],
  "author": "Glincker <hello@glincker.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/glincker/geokit"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "husky": "^9.1.7",
    "turbo": "^2.3.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
