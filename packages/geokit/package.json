{
  "name": "@glincker/geokit",
  "version": "0.2.2",
  "description": "Unified CLI and fix pipeline for GeoKit â€” audit, generate, convert, and fix your site's AI-readiness.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "geokit": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs",
    "dist/**/*.d.ts",
    "dist/**/*.d.cts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "geo",
    "seo",
    "ai",
    "geokit",
    "audit",
    "llms-txt",
    "generative-engine-optimization",
    "ai-seo",
    "ai-readiness",
    "structured-data",
    "json-ld",
    "robots-txt",
    "sitemap",
    "markdown",
    "url-to-markdown"
  ],
  "author": "Glincker <hello@glincker.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GLINCKER/geokit",
    "directory": "packages/geokit"
  },
  "homepage": "https://geo.glincker.com",
  "bugs": {
    "url": "https://github.com/GLINCKER/geokit/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@glincker/geo-audit": "*",
    "@glincker/geo-seo": "*",
    "@glincker/geomark": "*",
    "chalk": "^5.4.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "@types/node": "^22.0.0"
  }
}
