{
  "name": "@glincker/geo-seo",
  "version": "0.4.0",
  "description": "Framework-agnostic GEO asset generator â€” llms.txt, JSON-LD, robots.txt, and sitemap for AI discoverability.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "geo-seo": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/next.d.ts",
        "default": "./dist/next.js"
      },
      "require": {
        "types": "./dist/next.d.cts",
        "default": "./dist/next.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./astro": {
      "import": {
        "types": "./dist/astro.d.ts",
        "default": "./dist/astro.js"
      },
      "require": {
        "types": "./dist/astro.d.cts",
        "default": "./dist/astro.cjs"
      }
    },
    "./nuxt": {
      "import": {
        "types": "./dist/nuxt.d.ts",
        "default": "./dist/nuxt.js"
      },
      "require": {
        "types": "./dist/nuxt.d.cts",
        "default": "./dist/nuxt.cjs"
      }
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs",
    "dist/**/*.d.ts",
    "dist/**/*.d.cts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "geo",
    "seo",
    "ai",
    "llms-txt",
    "json-ld",
    "robots-txt",
    "sitemap",
    "generative-engine-optimization",
    "ai-seo",
    "schema-org",
    "structured-data",
    "nextjs",
    "vite",
    "astro",
    "nuxt"
  ],
  "author": "Glincker <hello@glincker.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GLINCKER/geokit",
    "directory": "packages/geo-seo"
  },
  "homepage": "https://geokit.dev",
  "bugs": {
    "url": "https://github.com/GLINCKER/geokit/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "vite": ">=4.0.0",
    "astro": ">=3.0.0",
    "nuxt": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    }
  }
}
